	<table id="map_table" cellpadding="3">

		<tr><td>Milestone Type</td>
			<td>
				<%= select("nepa_milestone", "nepa_lookup_milestone_id", NepaLookupMilestone.all.collect {|nlm| [ nlm.milestone, nlm.id ] }) %>

